{
  "cleanUrls": true,
  "rewrites": [
    {
      "source": "/",
      "destination": "/frontend/pages/index.html"
    },
    {
      "source": "/auth",
      "destination": "/frontend/pages/auth.html"
    },
    {
      "source": "/admin",
      "destination": "/frontend/pages/admin.html"
    },
    {
      "source": "/app",
      "destination": "/frontend/pages/app.html"
    },
    {
      "source": "/js/:file*",
      "destination": "/frontend/js/:file*"
    },
    {
      "source": "/images/:file*",
      "destination": "/frontend/images/:file*"
    }
  ]
}
